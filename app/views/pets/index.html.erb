<p style="color: green"><%= notice %></p>
<form action="http://localhost:3000/">
  <input type="submit" value="Home"/>
</form>
<h1>Pets</h1>

<div id="pets">
  <% @pets.each do |pet| %>
    <%= render pet %>
    <p>
      <%= link_to "Show this pet", pet %>
    </p>
  <% end %>
</div>

<%= link_to "New pet", new_pet_path %>
